{
    "php.validate.executablePath": "${workspaceFolder}/php/php.exe",
    "php.executablePath": "${workspaceFolder}/php/php.exe",
    "php.suggest.basic": false,
    "intelephense.environment.includePaths": [
        "vendor"
    ],
    "intelephense.diagnostics.undefinedTypes": true,
    "intelephense.diagnostics.undefinedMethods": true,
    "intelephense.diagnostics.undefinedConstants": true,
    "intelephense.diagnostics.undefinedClassConstants": true,
    "intelephense.diagnostics.undefinedFunctions": true,
    "files.associations": {
        "*.php": "php",
        ".bgl_core/brain/scenarios/generated/*.yaml": "plaintext",
        "**/.bgl_core/brain/scenarios/generated/*.yaml": "plaintext",
        ".bgl_core/brain/scenarios/generated/gap_route_agentfrontend_partials_agent_autonomy_state_php.yaml": "plaintext"
    },
    "files.exclude": {
        "**/.git": true,
        "**/node_modules": true
    },
    "search.exclude": {
        "**/vendor": true,
        "**/node_modules": true
    },
    "yaml.schemas": {
        "https://raw.githubusercontent.com/crowdsecurity/crowdsec-yaml-schemas/main/scenario_schema.yaml": [],
        "./.bgl_core/brain/scenarios/scenario_schema.json": [
            ".bgl_core/brain/scenarios/**/*.yaml"
        ]
    },
    "yaml.schemaStore.enable": false
}
